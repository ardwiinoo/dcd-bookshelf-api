{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": 0,
    "object-curly-spacing": 0,
    "require-jsdoc": 0,
    "max-len": 0,
    "indent": 0,
    "comma-dangle": 0,
    "operator-linebreak": 0
  }
}
